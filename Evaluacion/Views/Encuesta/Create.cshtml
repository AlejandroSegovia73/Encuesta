
@model EncuestaLibrery.Encuesta

@using (Html.BeginForm())
{
    <table>
        <tr>
            <td>Nombre de la encuesta</td>
            <td> @Html.EditorFor(model => model.equipo, new { htmlAttributes = new { @class = "form-control" } })</td>
        </tr>
        <tr>
            <td>Nombre del area</td>
            <td> @Html.EditorFor(model => model.nombreArea, new { htmlAttributes = new { @class = "form-control" } })</td>
        </tr>
        <tr>
            <td>
                <div class="form-group">
                    <div class="col-md-offset-2 col-md-10">
                        <input type="submit" value="Create" class="btn btn-default" />
                    </div>
                </div>
                <div class="btn btn-default">
                    @Html.ActionLink("Back to List", "Index")
                </div>

            </td>
        </tr>
    </table>

}

